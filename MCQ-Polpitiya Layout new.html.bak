<script type="text/javascript">
var total = 0;

//answer plugin
$.fn.answer = function() {
  this.click(onClick)
  function onClick() {
    var val = $(this).val();
    if (val == 1) {
      total++
      $(this).removeClass('btn-primary').addClass('btn-success').parent().append('<h2>The answer is correct!</h2>').find('button').unbind();;
    }
    else {
      $(this).removeClass('btn-primary').addClass('btn-danger').parent().append('<h3>The answer is wrong!</h3>').find('button').unbind();
    };

  };
};


// calculate the final score and display one of a range
$.fn.calcScore = function() {
  this.click(onClick)

  function onClick() {
    if (total == 0) {
      $('.score').append('<h2>No question is correct!</h2>');
      $('.first').slideDown();
    } else if (total > 0) {
      $('.score').append('<h2>Out of 40 questions ' + total + ' is correct!</h2>');
      $('.first').slideDown();
    }
    else if (total >= 5 && total <= 9) {
    };
    $(this).unbind();
  };
};

// call answer plugin on each question
$('.q1b').answer();

// call calcScore plugin on score button
$('.scoreButton').calcScore();

//PROTECT
document.addEventListener('contextmenu', event => event.preventDefault());
  </script>
  
